<div class="row">
  {{#each nota}}
  <div class="col-md-4">
    <div class="card">
      <div class="card-header">
        {{ titulo }}
      </div>
      <div class="card-body">
        <p>{{ contenido }} </p>
        <!-- DELETE REQUEST -->
        <form action="/nota/delete/{{_id}}" method="POST">
          <button type="submit" class="btn btn-danger btn-block btn-sm">Borrar</button>
        </form>
      </div>
      <div class="card-footer">
        {{redactor}}
      </div>
    </div>
  </div>
  {{/each}}
</div>
